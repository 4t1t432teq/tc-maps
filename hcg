--[[ Retro Ultimate GUI | By ChatGPT ]]
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local TweenService = game:GetService("TweenService")

-- GUI Setup
local screenGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
screenGui.Name = "UltimateRetroGUI"
screenGui.ResetOnSpawn = false

-- Boot-up Frame
local bootFrame = Instance.new("Frame", screenGui)
bootFrame.Size = UDim2.new(1,0,1,0)
bootFrame.BackgroundColor3 = Color3.new(0,0,0)

local bootLabel = Instance.new("TextLabel", bootFrame)
bootLabel.Size = UDim2.new(1,0,1,0)
bootLabel.Text = ">> Initializing Ultimate GUI..."
bootLabel.TextColor3 = Color3.new(0,1,0)
bootLabel.BackgroundTransparency = 1
bootLabel.TextScaled = true
bootLabel.Font = Enum.Font.Code

-- Dramatic Boot-Up Effect
task.spawn(function()
	local messages = {
		"> BIOS V1.98 Loading...",
		"> Memory Check: OK",
		"> Graphics Mode: Retro RGB",
		"> Loading Payload...",
		"> Ultimate GUI Ready. Press Any Button."
	}
	for _, msg in ipairs(messages) do
		bootLabel.Text = msg
		task.wait(1)
	end
	wait(0.5)
	TweenService:Create(bootFrame, TweenInfo.new(1), {BackgroundTransparency = 1}):Play()
	TweenService:Create(bootLabel, TweenInfo.new(1), {TextTransparency = 1}):Play()
	wait(1)
	bootFrame:Destroy()
end)

-- Main Frame
local mainFrame = Instance.new("Frame", screenGui)
mainFrame.Size = UDim2.new(0.3, 0, 0.6, 0)
mainFrame.Position = UDim2.new(0.05, 0, 0.2, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
mainFrame.BorderColor3 = Color3.new(0, 1, 0)

-- Scanline Effect
local scanlines = Instance.new("ImageLabel", mainFrame)
scanlines.Size = UDim2.new(1, 0, 1, 0)
scanlines.BackgroundTransparency = 1
scanlines.Image = "http://www.roblox.com/asset/?id=5553946656"
scanlines.ImageTransparency = 0.9
scanlines.ZIndex = 2

-- UIListLayout for buttons
local scrollingFrame = Instance.new("ScrollingFrame", mainFrame)
scrollingFrame.Size = UDim2.new(1,0,1,0)
scrollingFrame.CanvasSize = UDim2.new(0, 0, 2.5, 0)
scrollingFrame.ScrollBarThickness = 4
scrollingFrame.BackgroundTransparency = 1

local listLayout = Instance.new("UIListLayout", scrollingFrame)
listLayout.Padding = UDim.new(0, 4)

-- Button Creator Function
local function createButton(text, loadstringURL)
	local button = Instance.new("TextButton")
	button.Size = UDim2.new(1, -10, 0, 40)
	button.Position = UDim2.new(0,5,0,0)
	button.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
	button.BorderColor3 = Color3.new(0, 1, 0)
	button.TextColor3 = Color3.new(0,1,0)
	button.Font = Enum.Font.Code
	button.TextSize = 18
	button.Text = "â–¶ " .. text
	button.AutoButtonColor = false

	-- Hover Animation
	button.MouseEnter:Connect(function()
		TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
	end)
	button.MouseLeave:Connect(function()
		TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(20, 20, 20)}):Play()
	end)

	button.MouseButton1Click:Connect(function()
		pcall(function()
			loadstring(game:HttpGet(loadstringURL))()
		end)
	end)

	button.Parent = scrollingFrame
end

